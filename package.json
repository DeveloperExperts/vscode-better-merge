{
    "name": "better-merge",
    "displayName": "Better Merge",
    "description": "Improved git merge conflict support",
    "version": "0.1.5",
    "publisher": "pprice",
    "engines": {
        "vscode": "^1.5.0"
    },
    "icon": "img/app.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/pprice/vscode-better-merge.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "category": "Better Merge",
                "title": "Accept all ours (current file)",
                "command": "better-merge.accept.all-ours"
            },
            {
                "category": "Better Merge",
                "title": "Accept all theirs (current file)",
                "command": "better-merge.accept.all-theirs"
            },
            {
                "category": "Better Merge",
                "title": "Accept ours (current conflict)",
                "command": "better-merge.accept.ours"
            },
            {
                "category": "Better Merge",
                "title": "Accept theirs (current conflict)",
                "command": "better-merge.accept.theirs"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}